{
    "definition": {
      "openapi": "3.0.0",
      "info": {
        "title": "Sample API",
        "description": "https://driver.onrender.com/v1 and http://localhost:8080/v1",
        "version": "0.1.0"
      },
      "servers": [
        {
          "url": "http://localhost:8080/v1",
          "description": "local server"
        },
        {
          "url": "https://driver.onrender.com/v1",
          "description": "render server"
        }
      ],
      "paths": {
        "/user": {
          "post": {
            "tags": ["User"],
            "description": "This api create new user. Mobile phone send to the server(Yangi foydalanuvchini yaratadi. Mobile phone serverga yuboradi)",
            "requestBody": {
              "description": "Important elements in this body(Asosiy elementlar)",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            },
            "responses": {
              
            }
          }
        },
        "/user/login": {
          "post": {
            "tags": ["User"],
            "description": "Login User",
            "requestBody": {
              "description": "Login with email",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "required": ["email","password"],
                    "properties": {
                      "email": {
                        "type": "string",
                        "example": "string007@gamil.com"
                      },
                      "password": {
                        "type": "string",
                        "example": "string1234"
                      }
                    }
                  }
                }
              }
            },
            "responses": {
              
            }
          }
        },
        "/user/users": {
          "get": {
            "tags": ["User"],
            "description": "Device List",
            "parameters": [
              {
                "name": "x-auth-token",
                "in": "header",
                "description": "an authorization header",
                "type": "string"
              },
              {
                "name": "status",
                "in": "query",
                "description": "status",
                "type": "number"
              }
            ],
            "responses": {
              
            }
          }
        },
        "/device/login": {
          "post": {
            "tags": ["Device"],
            "description": "Login Device",
            "requestBody": {
              "description": "Login with email",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "required": ["device_number"],
                    "properties": {
                      "device_number": {
                        "type": "string",
                        "example": "string"
                      }
                    }
                  }
                }
              }
            },
            "responses": {
              
            }
          }
        },
        "/device": {
          "post": {
            "tags": ["Device"],
            "description": "Create device(Device yaratish)",
            "requestBody": {
              "description": "Important elements in this body(Asosiy elementlar)",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Device"
                  }
                }
              }
            },
            "responses": {
              
            }
          }
        },
        "/device/devices": {
          "get": {
            "tags": ["Device"],
            "description": "Device List",
            "parameters": [
              {
                "name": "x-auth-token",
                "in": "header",
                "description": "an authorization header",
                "type": "string"
              },
              {
                "name": "limit",
                "in": "query",
                "description": "question's category",
                "type": "number"
              },
              {
                "name": "page",
                "in": "query",
                "description": "question's category",
                "type": "number"
              }
            ],
            "responses": {
              
            }
          }
        },
        "/device/search": {
          "get": {
            "tags": ["Device"],
            "description": "Device List",
            "parameters": [
              {
                "name": "x-auth-token",
                "in": "header",
                "description": "an authorization header",
                "type": "string"
              },
              {
                "name": "limit",
                "in": "query",
                "description": "question's category",
                "type": "number"
              },
              {
                "name": "page",
                "in": "query",
                "description": "question's category",
                "type": "number"
              },
              {
                "name": "name",
                "in": "query",
                "description": "question's category",
                "type": "number"
              },
              {
                "device_number": "name",
                "in": "query",
                "description": "question's category",
                "type": "number"
              }
            ],
            "responses": {
              
            }
          }
        },
        "/device/remove": {
          "delete": {
            "tags": ["Device"],
            "description": "User Remove",
            "parameters": [
              {
                "name": "x-auth-token",
                "in": "header",
                "description": "an authorization header",
                "type": "string"
              },
              {
                "name": "_id",
                "in": "query",
                "description": "device id",
                "type": "string"
              }
            ],
            "responses": {
              
            }
          }
        },
        "/condition": {
          "post": {
            "tags": ["Condition"],
            "description": "This api create new user. Mobile phone send to the server(Yangi foydalanuvchini yaratadi. Mobile phone serverga yuboradi)",
            "parameters": [
              {
                "name": "x-auth-token",
                "in": "header",
                "description": "an authorization header",
                "type": "string"
              }
            ],
            "requestBody": {
              "description": "Important elements in this body(Asosiy elementlar)",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Condition"
                  }
                }
              }
            },
            "responses": {
              
            }
          }
        },
        "/condition/create-condition": {
          "post": {
            "tags": ["Condition"],
            "description": "This api create new user. Mobile phone send to the server(Yangi foydalanuvchini yaratadi. Mobile phone serverga yuboradi)",
            "parameters": [
              {
                "name": "x-auth-token",
                "in": "header",
                "description": "an authorization header",
                "type": "string"
              }
            ],
            "responses": {
              
            }
          }
        }
      },
      "components": {
        "schemas": {
          "User":{
            "type": "object",
            "required": ["name","email","password", "status", "tel_number", "device_number"],
            "properties": {
              "name": {
                "type": "string"
              },
              "email": {
                "type": "string"
              },
              "password": {
                "type": "string"
              },
              "status": {
                "type": "number"
              },
              "tel_number": {
                "type": "string"
              },
              "device_number": {
                "type": "string"
              }
            }
          },
          "Device": {
            "type": "object",
            "required": ["device_number", "name"],
            "properties": {
              "name": {
                "type": "string"
              },
              "device_number": {
                "type": "string"
              }
            }
          },
          "Condition": {
            "type": "object",
            "required": ["user", "device_number"],
            "properties": {
              "user": {
                "type": "string"
              },
              "truck": {
                "type": "string"
              },
              "device_number": {
                "type": "string"
              },
              "status": {
                "type": "number"
              },
              "created_at": {
                "type": "string",
                "format": "date-time",
                "example": "2023-01-30T08:30:00Z"
              },
              "updated_at": {
                "type": "string",
                "format": "date-time",
                "example": "2023-01-30T08:30:00Z"
              },
              "finished_at": {
                "type": "string",
                "format": "date-time",
                "example": "2023-01-30T08:30:00Z"
              }
            }
          }
        }
      }
    },
    "apis": ["./routes/user.js"]
  }